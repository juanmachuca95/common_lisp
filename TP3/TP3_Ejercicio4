(defun tp3ejer4()
	(let (a)
		(write "escribe una lista: ")
		(setq a (read))
		(if (and (consp a)(numberp (car a)) (numberp (car (last a))) (= (car a) (car (last a))))

			(pprint "son iguales")
			(pprint "no son iguales")
		)
	)
)

TP3EJER4
Break 25 [27]> (tp3ejer4)
"escribe una lista: "(2 2 3 4)

"no son iguales"

Break 25 [27]> (tp3ejer4)
"escribe una lista: "(2 2 3 2)

"son iguales"

