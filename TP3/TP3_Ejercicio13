(defun tp3ejer12 ()
	(let (a b)
		(write "ingresa lista a: ")
		(setq a(read))
		(write "ingresa lista b: ")
		(setq b(read))

		(if (and (consp a) (consp b))
			(length (setq a(append a b)))
		)
	)
)

